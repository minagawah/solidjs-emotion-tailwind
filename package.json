{
  "name": "solidjs-emotion-tailwind",
  "version": "1.4.0",
  "description": "Example webpack project using Solid.js with Emotion + Tailwind.",
  "main": "index.js",
  "scripts": {
    "lint": "$npm_execpath eslint src/**/*.js",
    "pretty": "$npm_execpath prettier -w \"src/**/*.js\"",
    "test": "$npm_execpath jest",
    "start": "$npm_execpath webpack-dev-server --open --config webpack.dev.js",
    "build": "rm -fR build/* && $npm_execpath webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minagawah/solidjs-emotion-tailwind.git"
  },
  "keywords": [
    "solidjs",
    "emotion",
    "tailwind"
  ],
  "author": "Hiroki Minagawa",
  "license": "(Unlicense OR MIT)",
  "private": false,
  "bugs": {
    "url": "https://github.com/minagawah/solidjs-emotion-tailwind/issues"
  },
  "homepage": "https://github.com/minagawah/solidjs-emotion-tailwind#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime-corejs3": "^7.11.2",
    "autoprefixer": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-preset-solid": "^0.19.3",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "3",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "jest-emotion": "^10.0.32",
    "license-webpack-plugin": "^2.3.0",
    "mini-css-extract-plugin": "^0.11.2",
    "postcss-loader": "^4.0.2",
    "prettier": "^2.1.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "@pixi/canvas-graphics": "^5.3.3",
    "emotion": "^10.0.27",
    "pixi.js": "^5.3.3",
    "pixi.js-legacy": "^5.3.3",
    "ramda": "^0.27.1",
    "solid-js": "^0.19.3",
    "tailwindcss": "^1.8.10",
    "twin.macro": "^1.9.0"
  }
}
